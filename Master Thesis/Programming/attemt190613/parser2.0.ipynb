    "from lxml import objectify\n",
    "import pandas as pd\n",
    "xml = objectify.parse('AuctionRoll10.xml')\n",
    "root = xml.getroot()\n",
    "\n",
    "data = []\n",
    "for i in range(len(root.getchildren())):\n",
    "    data.append([child.txt for child in root.getchildren()[i].getchildren()])\n",
    "\n",
    "df = pd.DataFrame(data).T\n",
    "print(df)"
